<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjectMadManga.Helpers.HomePage"
             IconImageSource="Options.png">
    <ContentPage.Content>
        <TableView Intent="Settings">
            <TableRoot Title="Options Menu">
                <TableSection>
                    <SwitchCell x:Name="PinkSwitch" Text="PinkMode"></SwitchCell>
                </TableSection>
            </TableRoot>
        </TableView>
    </ContentPage.Content>
</ContentPage>